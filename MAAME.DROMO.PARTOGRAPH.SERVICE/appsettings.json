{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=partograph.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "SyncSettings": {
    "MaxRecordsPerPull": 100,
    "SyncBatchSize": 50,
    "ConflictResolutionStrategy": "ServerWins"
  }
}
